<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="CodeFont">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/White</item>
        <item name="android:textSize">14dp</item>
    </style>

    <style name="CodeFont1">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/White</item>
        <item name="android:textSize">12dp</item>
    </style>

    <style name="CodeFontHist">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/White</item>
        <item name="android:textSize">9dp</item>
    </style>

    <style name="CodeFontHist1">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/White</item>
        <item name="android:textSize">9dp</item>
    </style>

    <style name="CodeFontPos">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/Wheat</item>
        <item name="android:textSize">11dp</item>
    </style>

    <style name="CodeFontOrder">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/Black</item>
        <item name="android:textSize">12dp</item>
    </style>

    <style name="CustomTheme" parent="@android:style/Theme">
        <item name="android:tabWidgetStyle">@style/CustomTabWidget</item>
    </style>

    <style name="CustomTabWidget" parent="@android:style/Widget.TabWidget">
        <item name="android:textAppearance">@style/CustomTabWidgetText</item>
    </style>

    <style name="CustomTabWidgetText" parent="@android:style/TextAppearance.Widget.TabWidget">
        <item name="android:textSize">10dp</item>
    </style>

    <style name="CodeFontSel1">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_gravity">center_vertical</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:textColor">@color/Yellow</item>
        <item name="android:textSize">16dp</item>
    </style>

    <style name="CodeFontSel2">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_gravity">center_vertical</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:textColor">@color/White</item>
        <item name="android:textSize">10dp</item>
    </style>

</resources>